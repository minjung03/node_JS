<% var name %>
<h1><%= name %></h1> <!-- 문자열 출력 -->

<p style="color:red;"><%= description %> </p>

<!-- 자바스크립트 코드 -->
<hr><% for(var i=1; i<10; i++) { %> 
<h3> 9 * <%= i %> =<%= 9*i %>  </h3>
<% } %>  

<table border="1" width="700">
    <% for(var i=2; i<10; i++) { %> 
    <tr>
        <% for(var j=1; j<10; j++) { %> 
        <td>
            <%= i %> * <%= j %> =<%= i*j %>
        </td>
        <% } %>  
        
    </tr>
    <% } %>  
</table>